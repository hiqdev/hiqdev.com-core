{
    "name": "hiqdev/hiqdev.com-core",
    "type": "yii2-extension",
    "description": "hiqdev.com site",
    "keywords": [
        "hiqdev",
        "site"
    ],
    "homepage": "https://github.com/hiqdev/hiqdev.com-core",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@hiqdev.com",
        "source": "https://github.com/hiqdev/hiqdev.com-core",
        "issues": "https://github.com/hiqdev/hiqdev.com-core/issues",
        "wiki": "https://github.com/hiqdev/hiqdev.com-core/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
            "homepage": "http://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "role": "Lead backend developer",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        },
        {
            "name": "Andrey Klochok",
            "role": "Lead frontend developer",
            "email": "andreyklochok@gmail.com",
            "homepage": "http://hiqdev.com/"
        },
        {
            "name": "Yuriy Myronchuk",
            "role": "QA Lead",
            "email": "bladeroot@gmail.com",
            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "yiisoft/yii2-debug": "~2.0.1",
        "yiisoft/yii2-swiftmailer": "~2.0.6",
        "mito/yii2-sentry": "^1.0.0",
        "creocoder/yii2-flysystem": "~0.8",
        "sadovojav/yii2-image-thumbnail": "dev-master",
        "hiqdev/hisite": "<2.0 || dev-master",
        "hiqdev/yii2-yandex-metrika": "<2.0 || dev-master",
        "hiqdev/yii2-hiart-github": "<2.0 || dev-master",
        "hiqdev/yii2-module-pages": "<2.0 || dev-master",
        "hiqdev/yii2-monitoring": "<2.0 || dev-master",
        "hiqdev/yii2-theme-hyde": "<2.0 || dev-master",
        "hiqdev/yii2-asset-hiqdev": "<2.0 || dev-master"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0 || dev-master",
        "hiqdev/hidev-hiqdev": "<2.0 || dev-master"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\com\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "params": "src/config/params.php",
            "common": "src/config/common.php",
            "console": "src/config/console.php",
            "web": "src/config/web.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
